<?xml version="1.0" encoding="utf-8"?>
<Dialog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Steps>
    <DialogStep Num="1" Question="Начинаем работу?" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="2" Response="Да" />
        <DialogResponse Next="23" Response="Нет" />
      </Responses>
    </DialogStep>
    <DialogStep Num="2" Question="Введите режим создания ПС (пс = программная система &amp; программное средство)" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="0" Response="Ручной" />
        <DialogResponse Next="0" Response="Автоматический" />
        <DialogResponse Next="0" Response="Автоматический ручной" />
      </Responses>
    </DialogStep>
    <DialogStep Num="3" Question="Введите режим подключения готовых модулей" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="0" Response="Исходный код" />
        <DialogResponse Next="0" Response="Загрузочный код" />
        <DialogResponse Next="0" Response="Сочетание загрузочного и исходного кода" />
      </Responses>
    </DialogStep>
    <DialogStep Num="4" Question="Введите основное руководство по созданию ПС" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="0" Response="ISO 12207:1995" />
        <DialogResponse Next="0" Response="ГОСТ 19.102­77" />
      </Responses>
    </DialogStep>
    <DialogStep Num="5" Question="Необходимо ли административное управление проектом?" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="6" Response="Да" />
        <DialogResponse Next="7" Response="Нет" />
      </Responses>
    </DialogStep>
    <DialogStep Num="6" Question="Введите руководство по управлению проектом" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="0" Response="ISO 16326:1999" />
      </Responses>
    </DialogStep>
    <DialogStep Num="7" Question="Начать конструирование?" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="8" Response="Нет" />
        <DialogResponse Next="0" Response="Да">
          <InnerDialog FormalModel="GameTheory">
            <Steps>
              <DialogStep Num="1" Question="Какие модули должны быть установлены вместе с программой?" Help="" Error="Неверный формат ввода">
                <Responses>
                  <DialogResponse Next="0" Response="JVM" />
                  <DialogResponse Next="0" Response=".NET" />
                </Responses>
              </DialogStep>
              <DialogStep Num="2" Question="Приложения какого типа нужно создать?" Help="" Error="Неверный формат ввода">
                <Responses>
                  <DialogResponse Next="0" Response="Windows Forms" />
                  <DialogResponse Next="0" Response="Консольное" />
                </Responses>
              </DialogStep>
              <DialogStep Num="3" Question="Следует ли подключать дополнительные библиотеки?" Help="" Error="Неверный формат ввода">
                <Responses>
                  <DialogResponse Next="0" Response="Да" />
                  <DialogResponse Next="0" Response="Нет" />
                </Responses>
              </DialogStep>
              <DialogStep Num="4" Question="Добавить поддерживаемую платформу?" Help="" Error="Неверный формат ввода">
                <Responses>
                  <DialogResponse Next="5" Response="Да" />
                  <DialogResponse Next="7" Response="Нет" />
                </Responses>
              </DialogStep>
              <DialogStep Num="5" Question="Какую платформу добавить?" Help="" Error="Неверный формат ввода">
                <Responses>
                  <DialogResponse Next="0" Response="Windows" />
                  <DialogResponse Next="0" Response="Linux" />
                  <DialogResponse Next="0" Response="Web" />
                  <DialogResponse Next="0" Response="OS X" />
                  <DialogResponse Next="0" Response="Android" />
                  <DialogResponse Next="0" Response="IOS" />
                  <DialogResponse Next="0" Response="Windows Phone" />
                </Responses>
              </DialogStep>
              <DialogStep Num="6" Question="Добавить дополнительную платформу?" Help="" Error="Неверный формат ввода">
                <Responses>
                  <DialogResponse Next="5" Response="Да" />
                  <DialogResponse Next="7" Response="Нет" />
                </Responses>
              </DialogStep>
              <DialogStep Num="7" Question="Следует ли использовать внешние фреймворки?" Help="" Error="Неверный формат ввода">
                <Responses>
                  <DialogResponse Next="0" Response="Да" />
                  <DialogResponse Next="0" Response="Нет" />
                </Responses>
              </DialogStep>
              <DialogStep Num="8" Question="Защитить проект от декомпиляции?" Help="" Error="Неверный формат ввода">
                <Responses>
                  <DialogResponse Next="0" Response="Да" />
                  <DialogResponse Next="0" Response="Нет" />
                </Responses>
              </DialogStep>
            </Steps>
          </InnerDialog>
        </DialogResponse>
      </Responses>
    </DialogStep>
    <DialogStep Num="8" Question="Система создана" Help="" Error="Неверный формат ввода">
      <Responses />
    </DialogStep>
    <DialogStep Num="9" Question="Необходимо тестирование системы?" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="13" Response="Нет" />
        <DialogResponse Next="10" Response="Да" />
      </Responses>
    </DialogStep>
    <DialogStep Num="10" Question="Введите руководство по тестированию" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="0" Response="ANSI/IEEE 1008­1986" />
        <DialogResponse Next="0" Response="ANSI/IEEE 1012­1986" />
      </Responses>
    </DialogStep>
    <DialogStep Num="12" Question="Система протестирована" Help="" Error="Неверный формат ввода">
      <Responses />
    </DialogStep>
    <DialogStep Num="13" Question="Необходимо документирование системы?" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="14" Response="Да" />
        <DialogResponse Next="17" Response="Нет" />
      </Responses>
    </DialogStep>
    <DialogStep Num="14" Question="Введите основные руководсва по документированию" Help="ГОСТ 19.105­78 &amp; ГОСТ 19.106­78 ­ для внутреннего использования |или| ISO 9294:1990 &amp; ISO 9127:1990 ­ для экспорта" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="0" Response="ГОСТ 19.105­78 &amp; ГОСТ 19.106­78" />
        <DialogResponse Next="0" Response="ISO 9294:1990 &amp; ISO 9127:1990" />
      </Responses>
    </DialogStep>
    <DialogStep Num="16" Question="Система задокументирована" Help="" Error="Неверный формат ввода">
      <Responses />
    </DialogStep>
    <DialogStep Num="17" Question="Необходимо ли оценить стоимость и качеств создаваемого ПС?" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="18" Response="Да" />
        <DialogResponse Next="21" Response="Нет" />
      </Responses>
    </DialogStep>
    <DialogStep Num="18" Question="Введите основное руководство оценки качества" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="0" Response="ISO 9126:1991" />
        <DialogResponse Next="0" Response="ISO 14598­1­6:1998­2000" />
      </Responses>
    </DialogStep>
    <DialogStep Num="23" Question="Завершить работу?" Help="" Error="Неверный формат ввода">
      <Responses>
        <DialogResponse Next="24" Response="Да" />
        <DialogResponse Next="1" Response="Нет" />
      </Responses>
    </DialogStep>
  </Steps>
</Dialog>